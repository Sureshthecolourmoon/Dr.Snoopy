<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <item android:id="@+id/circleprogress">
        
        <shape
            android:useLevel="false"
            android:shape="ring"
            >
            <solid android:color="@color/colorPrimaryDark"
                 />
            <gradient
                android:centerColor="#D43A6A"
                android:endColor="#FFC628"
                android:startColor="#F35B36"
                android:type="sweep"
                android:centerY="0.9"
                android:angle="270"
                android:useLevel="false"/>
        </shape>
    </item>

    </layer-list>